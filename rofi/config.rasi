configuration {
	modes: [ drun, filebrowser, window, run ];

	display-drun: "";
	display-filebrowser: "";
	display-window: "";
	display-run: "";

	drun-match-fields: "name, exec";
	drun-display-format: "{name}";
	window-match-fields: "";
	window-format: "{t}";
	filebrowser {
		directory: "/home";
	}

	show-icons: true;
	disable-history: true;
}
